log_enable(3,1);
SPARQL CLEAR GRAPH  <$ARGV[$I]graph/rao-stirling-index>;


DELETE FROM DB.DBA.load_list;
ld_dir ('/database/import', 'rao-stirling-dump.ttl', '$ARGV[$I]graph/rao-stirling-index');

